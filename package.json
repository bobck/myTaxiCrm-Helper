    "dealshrc": "ENV=TEST node -r dotenv/config src/bitrix/modules/get-and-save-deals-by-closed-date.mjs",
    "dealshrrs": "ENV=TEST node -r dotenv/config src/bitrix/modules/get-and-save-deals-rescheduled.mjs",
